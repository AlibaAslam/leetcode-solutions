#include <stdio.h>
#include <stdlib.h>

int mySqrt(int x) {
    if (x == 0) {
        return 0;
    }

    int left = 1;
    int right = x;
    int ans = 1;

    while (left <= right) {
        int mid = left + (right - left) / 2;
        
        
        long long mid_squared = (long long)mid * mid;

        if (mid_squared == x) {
            return mid;
        } else if (mid_squared < x) {
            ans = mid;
            left = mid + 1;
        } else {
            right = mid - 1;
        }
    }

    return ans;
}
